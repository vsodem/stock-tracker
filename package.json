{
  "name": "stock-tracker",
  "version": "1.0.0",
  "description": "A simple web app that tracks stock data in real time",
  "main": "app.js",
  "scripts": {
    "npm": "npm",
    "start": "node app.js",
    "dev": "npm run-script build && npm run-script serve-build",
    "serve": "cd tracker-src && ng serve -o",
    "build": "cd tracker-src && ng build --prod",
    "serve-build": "nodemon"
  },
  "author": "Vishal Sodem",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "request": "^2.81.0"
  }
}
